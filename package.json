{
  "name": "activity-page",
  "version": "0.0.2",
  "description": "Activity page template for svelte",
  "keywords": [
    "svelte",
    "sapper",
    "template",
    "svelte-template",
    "sapper-template",
    "activity-page"
  ],
  "author": {
    "name": "cnguu",
    "email": "www@cnguu.cn",
    "url": "https://github.com/cnguu"
  },
  "scripts": {
    "dev": "sapper dev -p 9527",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build -p 9528",
    "server": "serve -l 9529 __sapper__/export",
    "lint:eslint": "eslint --cache --max-warnings 0  \"src/**/*.{svelte,js}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,css,less,svelte,html,md}\"",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "clean:cache": "rimraf node_modules/.cache/",
    "clean:lib": "rimraf node_modules",
    "reload": "rimraf yarn.lock && rimraf package.lock.json && yarn clean:lib && yarn",
    "prepare": "husky install"
  },
  "repository": "https://github.com/spider-nest/activity-page.git",
  "homepage": "https://github.com/spider-nest/activity-page",
  "bugs": {
    "url": "https://github.com/spider-nest/activity-page/issues"
  },
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "polka": "^0.5.2",
    "sirv": "^1.0.17"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/runtime": "^7.15.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-url": "^6.1.0",
    "autoprefixer": "^10.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "husky": "^7.0.2",
    "less": "^4.1.1",
    "lint-staged": "^11.1.2",
    "lodash-es": "^4.17.21",
    "postcss": "^8.3.6",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.3.2",
    "prettier-plugin-svelte": "^2.4.0",
    "pretty-quick": "^3.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.29.2",
    "serve": "^12.0.1",
    "svelte": "^3.42.4",
    "svelte-preprocess": "^4.9.2"
  },
  "engines": {
    "node": "^12.20 || ^14.13.1 || >=16"
  }
}
